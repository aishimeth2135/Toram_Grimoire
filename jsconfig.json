{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
    },
    "noUnusedLocals": false
  },
  "include": [
		"src/**/*.js",
    "src/**/*.ts",
		"src/**/*.vue"
  ],
  "exclude": ["node_modules", "dist"],
  "typeAcquisition": {
    "exclude": ["dotenv"]
  }
}